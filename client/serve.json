{
    "rewrites": [
        {
            "source": "/tm/assets/:file",
            "destination": "/assets/:file"
        },
        {
            "source": "/tm/:path*",
            "destination": "/index.html"
        },
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/assets/**",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}